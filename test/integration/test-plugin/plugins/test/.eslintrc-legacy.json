{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "angular": "readonly",
        "$": "readonly",
        "Firebase": "readonly",
        "tinyMCE": "readonly",
        "tinymce": "readonly",
        "ace": "readonly",
        "ngGrid": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "off"
    }
}